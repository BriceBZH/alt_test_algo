<?php

class ScheduleEntry {

    public function __construct(public string $sampleId, public string $technicianId, public string $equipmentId, public Datetime $startTime, public Datetime $endTime, public string $priority) {
        $this->sampleId = $sampleId;
        $this->technicianId = $technicianId;
        $this->equipmentId = $equipmentId;
        $this->startTime = $startTime;
        $this->endTime = $endTime;
        $this->priority = $priority;
    }

}